type Material {
    id: ID!
    type: MaterialType
    name: String!
    status: MaterialStatus
    description: String
    usageRestrictions: UsageRestriction
    licenceType: LicenceType
    targetAudience: TargetAudience
    language: Language
    task: Task
    createDatetime: String!
    updateDatetime: String
    keywords: [Keyword!]
    reviews: [MaterialReview!]
}